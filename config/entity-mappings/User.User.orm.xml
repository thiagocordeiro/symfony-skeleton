<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping>
    <entity name="App\Domain\User\User" table="users" repository-class="App\Infra\User\DoctrineUserRepository">
        <id name="id" type="integer">
            <generator strategy="AUTO"/>
        </id>
        <field name="username" length="255"/>
        <field name="email" length="255"/>
    </entity>
</doctrine-mapping>
