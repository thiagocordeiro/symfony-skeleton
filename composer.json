{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.4.0",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ramsey/uuid": "^3.8",
    "symfony/console": "4.3.*",
    "symfony/flex": "^1.1",
    "symfony/framework-bundle": "4.3.*",
    "symfony/orm-pack": "^1.0",
    "symfony/yaml": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.2",
    "slevomat/coding-standard": "^5.0",
    "squizlabs/php_codesniffer": "^3.4",
    "symfony/browser-kit": "4.3.*",
    "symfony/process": "4.3.*",
    "symfony/profiler-pack": "^1.0",
    "symfony/var-dumper": "4.3.*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "cs:check": "vendor/bin/phpcs --colors -ps",
    "test:unit": "vendor/bin/phpunit --testdox --color=always --testsuite Unit",
    "test:int": "vendor/bin/phpunit --testdox --color=always --testsuite Integration",
    "tests": [
      "@cs:check",
      "@test:unit",
      "@test:int"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "config": {
    "sort-packages": true
  }
}
